<?xml version="1.0" encoding="UTF-8"?>
<validators>

	<field name="formulaProcess.name">
		<field-validator type="requiredstring">
			<message>${getText("ec.common.validate.asert.notnull", {"RM.propertydisplayName.randon1487295011793"})}</message>
		</field-validator>
		<field-validator type="stringlength">
			<param name="maxLength">256</param>
			<message>${getText("ec.common.validate.content.toolong", {"RM.propertydisplayName.randon1487295011793", "256"})}</message>
		</field-validator>
	</field>
	

	

	<field name="formulaProcess.longTime">
        <field-validator type="double">
           <message>${getText("ec.common.validate.double.format.error", {"RM.propertydisplayName.randon1487292241411"})}</message>
        </field-validator>
        <field-validator type="number_precision">
			<param name="precision">19</param>
			<param name="scale">6</param>
        	<message>${getText("ec.common.validate.double.format.error1", {"RM.propertydisplayName.randon1487292241411", 13, 6})}</message>
        </field-validator>
	</field>
	

	<field name="formulaProcess.procSort">
		<field-validator type="stringlength">
			<param name="maxLength">256</param>
			<message>${getText("ec.common.validate.content.toolong", {"RM.propertydisplayName.randon1539849930820", "256"})}</message>
		</field-validator>
	</field>
	

	<field name="formulaProcess.meno">
		<field-validator type="stringlength">
			<param name="maxLength">256</param>
			<message>${getText("ec.common.validate.content.toolong", {"RM.propertydisplayName.randon1487292273721", "256"})}</message>
		</field-validator>
	</field>
	

	<field name="formulaProcess.formulaId.id">
		<field-validator type="required">
			<message>${getText("ec.common.validate.asert.notnull", {"ec.common.ID"})}</message>
		</field-validator>
	</field>
	

	

	
<field name="workFlowVar.comment">
	<field-validator type="stringlength">
		<param name="maxLength">2000</param>
		<message>${getText("ec.common.validate.content.toolong.prefix")}&lt;b&gt;${getText("ec.view.dealadvice")}&lt;/b&gt;${getText("ec.common.validate.content.toolong.maxlength.is")} &lt;b&gt;2000&lt;/b&gt;</message>

	</field-validator>
</field>

</validators>
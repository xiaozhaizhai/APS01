			<message>${getText("ec.common.validate.asert.notnull", {"RM.propertydisplayName.randon1487330156499"})}</message>
		</field-validator>
		<field-validator type="stringlength">
			<param name="maxLength">256</param>
			<message>${getText("ec.common.validate.content.toolong", {"RM.propertydisplayName.randon1487330156499", "256"})}</message>

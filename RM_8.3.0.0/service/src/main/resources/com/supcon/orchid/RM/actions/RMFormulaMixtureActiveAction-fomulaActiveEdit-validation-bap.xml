<?xml version="1.0" encoding="UTF-8"?>
<validators>

	<field name="formulaMixtureActive.name">
		<field-validator type="stringlength">
			<param name="maxLength">200</param>
			<message>${getText("ec.common.validate.content.toolong", {"RM.propertydisplayName.randon1487811442421", "200"})}</message>
		</field-validator>
	</field>
	

	

	<field name="formulaMixtureActive.longTime">
        <field-validator type="double">
           <message>${getText("ec.common.validate.double.format.error", {"RM.propertydisplayName.randon1487811796153"})}</message>
        </field-validator>
        <field-validator type="number_precision">
			<param name="precision">19</param>
			<param name="scale">6</param>
        	<message>${getText("ec.common.validate.double.format.error1", {"RM.propertydisplayName.randon1487811796153", 13, 6})}</message>
        </field-validator>
	</field>
	

	<field name="formulaMixtureActive.ingredientsOrder">
        <field-validator type="int">
           <message>${getText("ec.common.validate.int.format.error", {"RM.propertydisplayName.randon1487812100211"})}</message>
        </field-validator>
        <field-validator type="number_precision">
			<param name="precision">10</param>
        	<message>${getText("ec.common.validate.double.format.error2", {"RM.propertydisplayName.randon1487812100211", 10})}</message>
        </field-validator>
	</field>
	

	

	

	<field name="formulaMixtureActive.meno">
		<field-validator type="stringlength">
			<param name="maxLength">2000</param>
			<message>${getText("ec.common.validate.content.toolong", {"RM.propertydisplayName.randon1487812510638", "2000"})}</message>
		</field-validator>
	</field>
	
<field name="workFlowVar.comment">
	<field-validator type="stringlength">
		<param name="maxLength">2000</param>
		<message>${getText("ec.common.validate.content.toolong.prefix")}&lt;b&gt;${getText("ec.view.dealadvice")}&lt;/b&gt;${getText("ec.common.validate.content.toolong.maxlength.is")} &lt;b&gt;2000&lt;/b&gt;</message>

	</field-validator>
</field>

</validators>
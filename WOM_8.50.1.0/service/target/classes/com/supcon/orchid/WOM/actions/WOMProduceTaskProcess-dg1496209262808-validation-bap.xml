<?xml version="1.0" encoding="UTF-8"?>
<validators>

	<field name="name">
		<field-validator type="stringlength">
			<param name="maxLength">256</param>
			<message>${getText("ec.common.validate.content.toolong", {"WOM.propertydisplayName.randon1489720649125", "256"})}</message>
		</field-validator>
	</field>
	

	

	<field name="longTime">
        <field-validator type="double">
           <message>${getText("ec.common.validate.double.format.error", {"WOM.propertydisplayName.randon1489720689233"})}</message>
        </field-validator>
        <field-validator type="number_precision">
			<param name="precision">19</param>
			<param name="scale">6</param>
        	<message>${getText("ec.common.validate.double.format.error1", {"WOM.propertydisplayName.randon1489720689233", 13, 6})}</message>
        </field-validator>
	</field>
	

	<field name="factoryId.id">
		<field-validator type="required">
			<message>${getText("ec.common.validate.asert.notnull", {"WOM.propertydisplayName.radion14192323937823455"})}</message>
		</field-validator>
	</field>
	

	<field name="remark">
		<field-validator type="stringlength">
			<param name="maxLength">2000</param>
			<message>${getText("ec.common.validate.content.toolong", {"WOM.propertydisplayName.randon1489720730887", "2000"})}</message>
		</field-validator>
	</field>
	

	
</validators>
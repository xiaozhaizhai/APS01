<?xml version="1.0" encoding="UTF-8"?>
<validators>

	

	<field name="produceTaskActive.name">
		<field-validator type="requiredstring">
			<message>${getText("ec.common.validate.asert.notnull", {"WOM.propertydisplayName.randon1490683872072"})}</message>
		</field-validator>
		<field-validator type="stringlength">
			<param name="maxLength">256</param>
			<message>${getText("ec.common.validate.content.toolong", {"WOM.propertydisplayName.randon1490683872072", "256"})}</message>
		</field-validator>
	</field>
	

	

	

	<field name="produceTaskActive.productId.id">
		<field-validator type="required">
			<message>${getText("ec.common.validate.asert.notnull", {"MESBasic.propertydisplayName.radion1415772644833"})}</message>
		</field-validator>
	</field>
	

	<field name="produceTaskActive.planNum">
		<field-validator type="required">
			<message>${getText("ec.common.validate.asert.notnull", {"WOM.propertydisplayName.randon1489730174515"})}</message>
		</field-validator>
		<field-validator type="double_plus">
		<message>${getText("ec.common.validate.double.plus.error", {"WOM.propertydisplayName.randon1489730174515"})}</message>
		</field-validator>
	</field>
	

	

	

	

	

	
<field name="workFlowVar.comment">
	<field-validator type="stringlength">
		<param name="maxLength">2000</param>
		<message>${getText("ec.common.validate.content.toolong.prefix")}&lt;b&gt;${getText("ec.view.dealadvice")}&lt;/b&gt;${getText("ec.common.validate.content.toolong.maxlength.is")} &lt;b&gt;2000&lt;/b&gt;</message>

	</field-validator>
</field>

</validators>
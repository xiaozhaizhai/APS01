<?xml version="1.0" encoding="UTF-8"?>
<validators>
		
		

	<field name="customerMaterial.code">
		<field-validator type="stringlength">
			<param name="maxLength">256</param>
			<message>${getText("ec.common.validate.content.toolong", {"MESBasic.propertydisplayName.randon1559177178994", "256"})}</message>
		</field-validator>
	</field>
	

	<field name="customerMaterial.name">
		<field-validator type="stringlength">
			<param name="maxLength">256</param>
			<message>${getText("ec.common.validate.content.toolong", {"MESBasic.propertydisplayName.randon1559177261248", "256"})}</message>
		</field-validator>
	</field>
	

	<field name="customerMaterial.customer.id">
		<field-validator type="required">
			<message>${getText("ec.common.validate.asert.notnull", {"MESBasic.propertydisplayName.randon1449640688865"})}</message>
		</field-validator>
	</field>
	

	<field name="customerMaterial.material.id">
		<field-validator type="required">
			<message>${getText("ec.common.validate.asert.notnull", {"MESBasic.propertydisplayName.radion1415772644833"})}</message>
		</field-validator>
	</field>
	

	
<field name="workFlowVar.comment">
	<field-validator type="stringlength">
		<param name="maxLength">2000</param>
		<message>${getText("ec.common.validate.content.toolong.prefix")}&lt;b&gt;${getText("ec.view.dealadvice")}&lt;/b&gt;${getText("ec.common.validate.content.toolong.maxlength.is")} &lt;b&gt;2000&lt;/b&gt;</message>

	</field-validator>
</field>

</validators>
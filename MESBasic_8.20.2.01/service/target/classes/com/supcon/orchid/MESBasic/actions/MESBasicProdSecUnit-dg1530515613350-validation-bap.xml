<?xml version="1.0" encoding="UTF-8"?>
<validators>

	

	

	<field name="changeRate">
		<field-validator type="required">
			<message>${getText("ec.common.validate.asert.notnull", {"MESBasic.propertydisplayName.randon1530515455954"})}</message>
		</field-validator>
        <field-validator type="double">
           <message>${getText("ec.common.validate.double.format.error", {"MESBasic.propertydisplayName.randon1530515455954"})}</message>
        </field-validator>
        <field-validator type="number_precision">
			<param name="precision">19</param>
			<param name="scale">6</param>
        	<message>${getText("ec.common.validate.double.format.error1", {"MESBasic.propertydisplayName.randon1530515455954", 13, 6})}</message>
        </field-validator>
	</field>
	

	<field name="demo">
		<field-validator type="stringlength">
			<param name="maxLength">2000</param>
			<message>${getText("ec.common.validate.content.toolong", {"MESBasic.propertydisplayName.randon1530515498015", "2000"})}</message>
		</field-validator>
	</field>
	
</validators>
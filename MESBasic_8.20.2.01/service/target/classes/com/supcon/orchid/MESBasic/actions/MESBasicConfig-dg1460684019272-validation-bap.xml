<?xml version="1.0" encoding="UTF-8"?>
<validators>

	<field name="name">
		<field-validator type="requiredstring">
			<message>${getText("ec.common.validate.asert.notnull", {"MESBasic.propertydisplayName.radion1416558524615"})}</message>
		</field-validator>
		<field-validator type="stringlength">
			<param name="maxLength">200</param>
			<message>${getText("ec.common.validate.content.toolong", {"MESBasic.propertydisplayName.radion1416558524615", "200"})}</message>
		</field-validator>
	</field>
	

	<field name="measurement.id">
		<field-validator type="required">
			<message>${getText("ec.common.validate.asert.notnull", {"MESBasic.propertydisplayName.randon1429853864414"})}</message>
		</field-validator>
	</field>
	

	<field name="formula">

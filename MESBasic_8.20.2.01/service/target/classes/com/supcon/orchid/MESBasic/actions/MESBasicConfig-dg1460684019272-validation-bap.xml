			<message>${getText("ec.common.validate.content.toolong", {"MESBasic.propertydisplayName.randon1426644791679", "2000"})}</message>
		</field-validator>
	</field>
	

	<field name="meter.id">
		<field-validator type="required">
			<message>${getText("ec.common.validate.asert.notnull", {"MESBasic.propertydisplayName.radion1415261315358"})}</message>
		</field-validator>
	</field>
	


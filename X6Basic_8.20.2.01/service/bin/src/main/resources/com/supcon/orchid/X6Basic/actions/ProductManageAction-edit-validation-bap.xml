	<field name="productManage.currQuantity">
        <field-validator type="double">
           <message>${getText("ec.common.validate.double.format.error", {"X6Basic.propertydisplayName.radion1371468561988"})}</message>
        </field-validator>
        <field-validator type="number_precision">
			<param name="precision">19</param>
